          "_name": "features07",
          "_sourceTheme": "carm4",
          "_customHTML": "<section class=\"features07 carm4_features07\" group=\"Features\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax, 'mbr-fullscreen': fullScreen}\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"9\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"4\" condition=\"fullScreen == false\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubTitle\">\n        <input type=\"checkbox\" title=\"Main Title\" name=\"showMaintitle\" checked>\n        <input type=\"checkbox\" title=\"Cards Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Icons\" name=\"showIcons\">\n        <input type=\"checkbox\" title=\"Cards Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Links\" name=\"showLinks\" checked>\n        <input type=\"checkbox\" title=\"Price Labels\" name=\"showPriceLabels\">\n        <select title=\"Cards\" name=\"cardsAmount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\" selected>2</option>\n            <option value=\"3\">3</option>\n        </select>\n        <input type=\"color\" title=\"Price Label Bg\" name=\"priceLabelBgColor\" value=\"#1352ff\" condition=\"showPriceLabels\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" value=\"../_images/background3.jpg\" title=\"Image\">\n            <input type=\"color\" value=\"#181818\" title=\"Color\" selected>\n            <input type=\"video\" value=\"https://www.youtube.com/watch?v=9morbKMwCu4\" title=\"Video\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div mbr-class=\"{'container-fluid': fullWidth, 'container': !fullWidth }\">\n        <div class=\"row justify-content-center\">\n            <div class=\"col-md-12\">\n                <h3 class=\"mbr-section-subtitle align-center mbr-white mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showSubTitle\" data-app-selector=\".mbr-section-subtitle, .mbr-section-btn\">Buy and sell\n                </h3>\n                <h2 class=\"mbr-section-title pb-2 mbr-bold mbr-white mbr-fonts-style align-left\" mbr-theme-style=\"display-2\" mbr-if=\"showMaintitle\" data-app-selector=\".mbr-section-title, .line-wrap\">Hands-on Labs</h2>\n            </div>\n        </div>\n        <div class=\"row row-content justify-content-center\">\n            <div class=\"card p-3 col-12 col-md-6\" mbr-class=\"{'col-lg-4': cardsAmount == '3'|| cardsAmount =='5'|| cardsAmount== '6'}\" mbr-if=\"cardsAmount > 0\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-img\">\n                        <a href=\"https://evilcorp.biz/\" target=\"_blank\"><img src=\"@PROJECT_PATH@/assets/images/screen-shot-2020-12-29-at-8.31.54-pm-1076x685.png\" title alt=\"Evil Corp\"></a>\n                        <div class=\"price-label\" mbr-if=\"showPriceLabels\">\n                            <p class=\"mbr-text mbr-fonts-style price-text\" mbr-theme-style=\"display-4\" data-app-selector=\".card .card-wrapper .card-img .price-label .price-text\">&nbsp;</p>\n                        </div>\n                    </div>\n                    <div class=\"card-box\">\n                        <h4 class=\"card-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\" data-app-selector=\".card .card-wrapper .card-box .card-title\">\n                            Evil Corp</h4>\n                        <div class=\"container-nested\" mbr-if=\"showIcons\">\n                            <div class=\"item\">\n                                <span mbr-icon class=\"mbr-iconfont mbri-calendar\"></span>\n                                <p class=\"mbr-text mbr-fonts-style icon-text\" mbr-theme-style=\"display-4\" data-app-selector=\".card .card-wrapper .card-box .container-nested .item .mbr-text, .mbr-section-btn\">\n                                    2025\n                                </p>\n                            </div>\n                            <div class=\"item\">\n                                <span mbr-icon class=\"mbr-iconfont mbri-setting\"></span>\n                                <p class=\"mbr-text mbr-fonts-style icon-text\" mbr-theme-style=\"display-4\" data-app-selector=\".card .card-wrapper .card-box .container-nested .item .mbr-text, .mbr-section-btn\">\n                                    Petrol\n                                </p>\n                            </div>\n                            <div class=\"item\">\n                                <span mbr-icon class=\"mbr-iconfont mbri-setting2\"></span>\n                                <p class=\"mbr-text mbr-fonts-style icon-text\" mbr-theme-style=\"display-4\" data-app-selector=\".card .card-wrapper .card-box .container-nested .item .mbr-text, .mbr-section-btn\">